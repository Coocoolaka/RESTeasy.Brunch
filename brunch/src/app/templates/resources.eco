<div id="resources">
	<h1> Resources of <%= @specName %> (<%= @specVersion %>)</h1>
		<%- "<a href=\"##{@specName}/#{@specVersion}/createResource\"> Create resource </a>" %>
		<% if @resources.length > 0: %>
			<ol>
				<% for resource in @resources.models: %>
					<li> <%= resource.get('url') %> </li>
				<% end %>
		<% else: %>
			<p> No resources defined </p>
		<% end %>
</div>